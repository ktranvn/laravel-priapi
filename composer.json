{
    "name": "ktran/priapi",
    "keywords": ["laravel", "ktran", "api"],
    "description": "PRIAPI - API Testing tool for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin",
            "email": "tranthai@enjoyworks.co.kr"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6|^7|^8",
        "maatwebsite/excel": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Ktran\\Priapi\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ktran\\Priapi\\PriapiServiceProvider"
            ],
            "aliases": {
                "Priapi": "Ktran\\Priapi"
            }
        }
    },

    "minimum-stability": "dev"
}
